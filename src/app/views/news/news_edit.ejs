<% include ../header %>

<pre><% news %></pre>

<div class="col-md-6 mt-4 card">
    <form class="card-body" action="/news/update/<%= news.id_news %>" method="post">
      <h3 class="card-title">Editar Noticias</h3>

        <div class="form-group">
        
        <input name="title" type="text" class="form-control" value=<%= news.title %> >
      </div>
      <div class="form-group">
        <input name="news" type="text" class="form-control" value=<%= news.news %>>
      </div>
      <input type="submit" class="btn btn-primary">
    </form>
  </div>
</div>

<% include ../footer %>